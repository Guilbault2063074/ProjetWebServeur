@model IEnumerable<Quiz>

<h1>Voir vos quiz</h1>
<ul>
<!--Pour chaque Quiz-->
@foreach (var qz in Model)
{
     <li>
         <a asp-action="PlayQuiz" asp-controller="Home" asp-route-quizId="@qz.QuizId">Quiz @qz.QuizId</a>
     </li>
}
</ul>

<br />
<button type="button" onclick="location.href='@Url.Action("StartScreen", "Home")'">Go Back to Start Screen</button>