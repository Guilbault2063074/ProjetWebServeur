@model IEnumerable<Quiz>

<ul>
<!--Pour chaque Quiz-->
@foreach (var qz in Model)
{
     <li>
         <a asp-action="SeeQuiz" asp-controller="Home" asp-route-quizId="@qz.QuizId">Quiz @qz.QuizId</a>
     </li>
}
</ul>
<br />
<a asp-action="NewQuiz" asp-controller="Home" class="border-1">Create a new Quiz</a>